{
  "name": "@kefah/nativescript-uuid",
  "version": "1.0.1",
  "description": "Get the UUID for IOS and Android",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "6.0.0",
      "android": "6.0.0"
    },
    "hooks": [
      {
        "type": "after-prepare",
        "script": "scripts/after-prepare.js",
        "inject": true
      }
    ]
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kefahB/nativescript-uuid.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "TypeScript",
    "iOS",
    "Android"
  ],
  "author": "KefahB <kefah.bader@gmail.com>",
  "bugs": {
    "url": "https://github.com/kefahB/nativescript-uuid/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/kefahB/nativescript-uuid",
  "bootstrapper": "@kefah",
  "dependencies": {
    "nativescript-hook": "^0.2.5"
  },
  "devDependencies": {}
}
